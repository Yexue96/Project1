<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-4.3.xcd">

    <bean id="student" class="com.project1.entity.Student" p:id="1" p:name="王子" p:age="22" p:address-ref="address"></bean>

    <bean id="address" class="com.project1.entity.Address" p:id="2" p:name="科技路"></bean>


</beans>
